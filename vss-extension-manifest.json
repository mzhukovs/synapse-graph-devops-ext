{
    "manifestVersion": 1,
    "id": "synapse-graph-extension",
    "name": "Synapse Graph",
    "version": "0.90.8",
    "publisher": "mark-zhukovsky",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "description": "Tools for building/releasing with Azure Synapse. Includes one pipeline task.",
    "repository": {
        "type": "git",
        "uri": "https://github.com/mzhukovs/synapse-graph-devops-ext"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "synapse",
        "dependencies",
        "graph"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "icons": {
        "default": "images/synapse_graph_128.png"
    },
    "branding": {
        "color": "rgb(116, 202, 231)",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/synapse_graph_screenshot.png"
        }
    ],
    "files": [
        {
            "path": "synapse_graph_task"
        }
    ],
    "contributions": [
        {
            "id": "synapse-graph-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "synapse_graph_task"
            }
        }
    ]
}